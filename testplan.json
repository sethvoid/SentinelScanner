{
  "test1": {
    "name": "Strict Transport Security",
    "type": "single-key-value",
    "matching-type": "contain",
    "key": "strict-transport-security",
    "value": {
      "max-age": "31536000"
    },
    "link": "https://developer.mozilla.org/en-US/docs/Web/HTTP/Headers/Strict-Transport-Security"
  },
